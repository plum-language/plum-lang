углоби "zadruga/sorte".

сорта Доручак {
    тури јаја = бунар.

    (јаја) {
        ја:јаја = јаја.
    }

    посо испржи комада {
        ако (комада == 0 или комада == бунар) {
            бекни "Испржено #{јаја} комада".
        } иначе {
            бекни "Испржено #{комада} комада".
        }
    }
}

посо газда {
    тури оброк = Доручак(5).
    оброк:испржи(5).

    врни 0.
}
